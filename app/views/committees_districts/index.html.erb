<h1>Wyniki komitetów w okręgu 
</h1>

<table>
  <thead>
    <tr>
      <th>Okręg</th>
      <th>Województwo</th>
      <th>Komitet</th>	
      <th>Votes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @committees_districts.each do |committees_district| %>
      <tr>
	<td><%= committees_district.district.name %></td>
	<td><%= committees_district.district.voivodship.name %></td>
	<td><%= committees_district.committee.name %></td>
        <td><%= committees_district.votes %></td>
        <td><%= link_to 'Edit', edit_committees_district_path(committees_district) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


