<h1>Dane dla okręgu</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Eligible voters</th>
      <th>Valid votes</th>
      <th>Invalid votes</th>
      <th>Vote cards</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
        <td><%= district.name %></td>
        <td><%= district.eligible_voters %></td>
        <td><%= district.valid_votes %></td>
        <td><%= district.invalid_votes %></td>
        <td><%= district.puste %></td>
	<td><%= district.wiele %></td>
	<td><%= district.inne %></td>
        <td><%= district.vote_cards %></td>
        <td><%= link_to 'Show', district %></td>
        <td><%= link_to 'Edit', edit_district_path(district) %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a href="/committees_districts">Uzupełnij wyniki poszczególnych komitetów</a>
