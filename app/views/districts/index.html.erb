<h1>Dane dla okręgu <%= current_user.district.name %></h1>

<table>
  <thead>
    <tr>
      
      <th>Uprawnieni do głosowania</th>
      <th>Ilość głosów ważnych</th>
      <th>Głosy nieważne</th>
      <th>Wydane karty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @districts.each do |district| %>
      <tr>
       
        <td><%= district.eligible_voters %></td>
        <td><%= district.valid_votes %></td>
        <td><%= district.invalid_votes %></td>
        <td><%= district.vote_cards %></td>
        <td><%= link_to 'Szczegóły', district %></td>
        <td><%= link_to 'Zaktualizuj', edit_district_path(district) %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a href="/committees_districts">Uzupełnij wyniki poszczególnych komitetów</a>
